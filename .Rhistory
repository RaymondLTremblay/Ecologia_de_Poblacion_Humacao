r <- 0.1
N0 <- 100
nyears <- 50
N0 * (1+r)
years <- seq(from=0, to=nyears, by=1)
years
N <- numeric(nyears)    # make an empty storage vector
N
N[1] <- N0
lambda <- 1 + r           # (1 + r) is equal to lambda, the finite rate of growth
for (i in 2:nyears){
N[i] <- lambda*N[i-1]
}
N
plot(N~years)
plot(N~years[-1])
plot(N~years[-1],type="l",col="green",lwd=3)
plot(N~years[-1])
N <- numeric(nyears+1)    # make an empty storage vector
N
N[1] <- N0
lambda <- 1 + r           # (1 + r) is equal to lambda, the finite rate of growth
for (i in 2:(nyears+1)){
N[i] <- lambda*N[i-1]
}
N
plot(N~years)
