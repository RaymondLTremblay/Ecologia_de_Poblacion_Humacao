out.curr <- tdat[[length(tdat)]]
if(backward) {
X0=head(out.curr,1);
t0=X0[1];
x0=X0[-1];
times=t0+seq(0,-maxtime,length=Tlen);
out=ode(times=times,y=x0,func=as.ode.func(fun),parms=parms,...);
return(rbind(out[nrow(out):2,],out.curr));
} else {
X0=tail(out.curr,1);
t0=X0[1]
x0=X0[-1]
times=t0+seq(0,maxtime,length=Tlen);
out=ode(times=times,y=x0,func=as.ode.func(fun),parms=parms,...);
return(rbind(out.curr,out[-1,]));
}
})
